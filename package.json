{
  "name": "logseq-block-extractor",
  "version": "0.3.1",
  "description": "Extract and filter blocks with hierarchical structure",
  "type": "module",
  "main": "dist/index.js",
  "logseq": {
    "id": "logseq-block-extractor",
    "title": "Block Extractor",
    "icon": "./icon.png"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist && rm -f logseq-block-extractor.zip",
    "dev": "npm run clean && vite",
    "build": "npm run clean && tsc && vite build",
    "dist": "npm run build && cd dist && zip -r ../logseq-block-extractor.zip .",
    "preview": "vite preview",
    "watch": "npm run clean && vite build --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inchanS/logseq-block-extractor.git"
  },
  "keywords": [
    "logseq",
    "plugin",
    "references",
    "blocks",
    "filter",
    "export"
  ],
  "author": "Inchan Song",
  "license": "MIT",
  "dependencies": {
    "@logseq/libs": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
